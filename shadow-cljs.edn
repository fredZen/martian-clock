{:deps true
 :dev-http {8080 "target/"}
 :builds {:app {:output-dir "target/"
                :asset-path "."
                :target :browser
                :modules {:main {:init-fn app.main/main!}}
                :devtools {:after-load app.main/reload!
                           :http-root "target"
                           :http-port 8080}}
          :test
          {:target    :node-test
           :output-to "out/node-tests.js"
           :autorun   true}}}
